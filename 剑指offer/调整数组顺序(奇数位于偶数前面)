# -*- coding:utf-8 -*-
class Solution:
    def reOrderArray(self, array):
        # write code here
        a = []
        b = []
        for i in range(len(array)):
            if array[i]%2 == 1:
                a.append(array[i])
            else:
                b.append(array[i])
        for i in range(len(b)):
            a.append(b[i])
        return a
        
